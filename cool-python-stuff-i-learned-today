command = ['bash', '-c', 'source config.sh && env']
proc = subprocess.Popen(command, stdout = subprocess.PIPE)
for line in proc.stdout:
    (key, _, value) = line.partition("=")
    os.environ[key] = value
proc.communicate()
pprint.pprint(dict(os.environ))
